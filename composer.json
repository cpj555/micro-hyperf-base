{
    "name": "losingbattle/micro-base",
    "description": "hyperf micro-base",
    "authors": [
        {
            "name": "cpj",
            "email": "741696717@qq.com"
        }
    ],
    "keywords": [
        "php",
        "hyperf",
        "microservice",
        "base"
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.4.0",
        "ext-json": "*",
        "ext-bcmath": "*",
        "ext-redis": "*",
        "hyperf/http-server": "2.2.*",
        "hyperf/context": "2.2.*",
        "hyperf/utils": "2.2.*",
        "hyperf/di": "2.2.*",
        "hyperf/logger": "2.2.*",
        "hyperf/db-connection": "2.2.*",
        "hyperf/database": "2.2.*",
        "inhere/php-validate": "^2.7",
        "hyperf/redis":"2.2.*",
        "hyperf/config": "2.2.*",
        "hyperf/service-governance": "2.2.*",
        "hyperf/snowflake": "2.2.*",
        "ramsey/uuid": "^4.1"
    },
    "require-dev": {
        "swoole/ide-helper": "^4.5"
    },
    "autoload": {
        "files": [
            "src/Functions.php"
        ],
        "psr-4": {
            "Losingbattle\\MicroBase\\": "src/"
        }
    },
    "extra": {
        "hyperf": {
            "config": "Losingbattle\\MicroBase\\ConfigProvider"
        }
    }
}
